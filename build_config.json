{
  "builds": {
    "vllm-cpu": {
      "description": "vLLM CPU inference engine (without AVX512 support, Runs on both x86_64 and Arm64 Processors)",
      "package_name": "vllm-cpu",
      "readme_file": "noavx512_README.md",
      "flags": {
        "disable_avx512": true,
        "enable_avx512vnni": false,
        "enable_avx512bf16": false,
        "enable_amxbf16": false
      },
      "platforms": ["x86_64", "aarch64"],
      "keywords": ["vllm", "llm", "inference", "cpu", "noavx512"]
    },
    "vllm-cpu-avx512": {
      "description": "vLLM CPU inference engine (AVX512 optimized)",
